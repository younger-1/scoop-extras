{
    "version": "3.22.3",
    "description": "SIP softphone for Windows based on PJSIP stack",
    "homepage": "https://www.microsip.org",
    "license": "GPL-2.0-only",
    "url": "https://www.microsip.org/download/MicroSIP-Lite-3.22.3.zip",
