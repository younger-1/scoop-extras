    "architecture": {
        "64bit": {
            "url": "https://github.com/dnGrep/dnGrep/releases/download/v4.5.66.0/dnGREP.4.5.66.x64.msi",
            "hash": "da45e7baf3620674dd924628ff630b1c43652d7f4b0bb122e105a982d2564876"
        },
        "32bit": {
            "url": "https://github.com/dnGrep/dnGrep/releases/download/v4.5.66.0/dnGREP.4.5.66.x86.msi",
            "hash": "e0ac69d5b4a60b5e62c599dad07c2a8fb50076d876fe35cd628a74cdd724e9af"
        }
    },
    "pre_install": "if (!(Test-Path \"$persist_dir\\dnGREP.Settings.dat\")) { New-Item \"$dir\\dnGREP.Settings.dat\" | Out-Null }",
    "extract_dir": "dnGREP",
    "bin": "dnGREP.exe",
    "shortcuts": [
        [
            "dnGREP.exe",
            "dnGREP"
        ]
    ],
    "persist": [
        "Themes",
        "dnGREP.Settings.dat"
    ],
    "checkver": {
        "github": "https://github.com/dnGrep/dnGrep"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/dnGrep/dnGrep/releases/download/v$version/dnGREP.$matchHead.x64.msi",
                "hash": {
                    "url": "https://github.com/dnGrep/dnGrep/releases/v$version",
                    "regex": "x64.msi[<>/\\w\\n: \\(\\)]+SHA256: ($sha256)"
                }
            },
            "32bit": {
                "url": "https://github.com/dnGrep/dnGrep/releases/download/v$version/dnGREP.$matchHead.x86.msi",
                "hash": {
                    "url": "https://github.com/dnGrep/dnGrep/releases/v$version",
                    "find": "x86.msi[<>/\\w\\n: \\(\\)]+SHA256: ($sha256)"
                }
            }
        }
    }
}
