    "homepage": "http://dngrep.github.io/",
    "license": "GPL-3.0-only",
    "architecture": {
        "64bit": {
            "url": "https://github.com/dnGrep/dnGrep/releases/download/v4.6.7.0/dnGREP.4.6.7.x64.msi",
            "hash": "ab36625195b441ce30f7696d8a6236b5d7883713ed87432e0459ee300f468adc"
        },
        "32bit": {
            "url": "https://github.com/dnGrep/dnGrep/releases/download/v4.6.7.0/dnGREP.4.6.7.x86.msi",
            "hash": "64469472330405aa7ed9084f0364721459583ad00d55e6cb0d1e01d03db543d1"
        }
    },
    "pre_install": "if (!(Test-Path \"$persist_dir\\dnGREP.Settings.dat\")) { New-Item \"$dir\\dnGREP.Settings.dat\" | Out-Null }",
    "extract_dir": "dnGREP",
    "bin": "dnGREP.exe",
    "shortcuts": [
        [
            "dnGREP.exe",
            "dnGREP"
        ]
    ],
    "persist": [
        "Themes",
        "dnGREP.Settings.dat"
    ],
    "checkver": {
        "github": "https://github.com/dnGrep/dnGrep"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/dnGrep/dnGrep/releases/download/v$version/dnGREP.$matchHead.x64.msi",
                "hash": {
                    "url": "https://github.com/dnGrep/dnGrep/releases/v$version",
                    "regex": "x64.msi[<>/\\w\\n: \\(\\)]+SHA256: ($sha256)"
                }
            },
            "32bit": {
                "url": "https://github.com/dnGrep/dnGrep/releases/download/v$version/dnGREP.$matchHead.x86.msi",
                "hash": {
                    "url": "https://github.com/dnGrep/dnGrep/releases/v$version",
                    "find": "x86.msi[<>/\\w\\n: \\(\\)]+SHA256: ($sha256)"
                }
            }
        }
    }
}
